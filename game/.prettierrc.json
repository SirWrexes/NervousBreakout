{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "trailingComma": "es5",
  "printWidth": 80,
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "arrowParens": "avoid",
  "objectWrap": "preserve",
  "endOfLine": "auto",
  "experimentalTernaries": true,
  "experimentalOperatorPosition": "start",
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}

{
  "author": "SirWrexes",
  "description": "Breakout game made with LÃ–VE",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@typescript-to-lua/language-extensions": "^1.19.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "love-typescript-definitions": "^11.5.0",
    "lua-types": "^2.13.1",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.8.3",
    "typescript-to-lua": "^1.31.0"
  },
  "engines": {
    "node": ">=22.7.0"
  },
  "keywords": [
    "love2d",
    "love",
    "lua",
    "typescript"
  ],
  "license": "ISC",
  "main": "main.ts",
  "name": "nervous-breakout",
  "packageManager": "pnpm@10.10.0",
  "scripts": {
    "clean": "node ./scripts/clean.js",
    "compile": "tstl",
    "recompile": "pnpm clean && pnpm compile",
    "build": "cd ./lua && makelove --config ../make.toml",
    "rebuild": "pnpm recompile && pnpm build",
    "watch": "tstl --watch",
    "start": "love ./lua",
    "restart": "pnpm recompile && love ./lua",
    "dev": "love --console ./lua --dev",
    "redev": "pnpm recompile && pnpm --dev"
  },
  "version": "1.0.0"
}
